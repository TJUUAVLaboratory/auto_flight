<launch>
	<!-- vim: set ft=xml noet : -->
	<!-- example launch script for PX4 based FCU's -->
	<!-- fcu　连接方式　USB口　ttyACM0　　　　　上面的数传串口　T1/T2   ttyUSB0 因为板载的FTDI 接在TK1的ttyUSB0 -->
	<!--<arg name="fcu_url" default="/dev/ttyACM0:921600" />-->
	<!--<arg name="fcu_url" default="/dev/ttyUSB0:57600" />-->
	<!--<arg name="gcs_url" default="udp://:14540@127.0.0.1:14550" />-->
	<!--启动mavros节点-->
	<arg name="fcu_url" default="udp://:14540@127.0.0.1:14557"/>
	<arg name="gcs_url" default="" />
	<arg name="tgt_system" default="1" />
	<arg name="tgt_component" default="1" />
	<arg name="log_output" default="screen" />
	<!-- <include file="$(find mavros)/launch/node.launch"><arg name="pluginlists_yaml" value="$(find mavros)/launch/px4_pluginlists.yaml" /><arg name="config_yaml" value="$(find mavros)/launch/px4_config.yaml" /><arg name="fcu_url" value="$(arg fcu_url)" /><arg name="gcs_url" value="$(arg gcs_url)" /><arg name="tgt_system" value="$(arg tgt_system)" /><arg name="tgt_component" value="$(arg tgt_component)" /><arg name="log_output" value="$(arg log_output)" /></include> -->
	<!--加载可视化rviz-->
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find auto_flight)/launch/guiji_map.rviz"/>
</launch>